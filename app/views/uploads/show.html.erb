                   			  <!-- ad goes here -->
 			</div>
           
 			</div>
             <div class="">
            <h4 style="color:white;"><%= @upload.text %></h4>
        </div>
        <hr>
    		<div class=" editthings" style="margin-top:  15px;" >
	  	<% if can? :destroy, @upload %>
			  			<%= link_to "Edit", edit_upload_path(@upload) %>   
					<% end %>
					<% if can? :destroy, @upload %>
			    		<%= link_to "Destroy", @upload, :method => :delete, :confirm => "Are you sure you want to delete?" %> 
			    		<span><strong>Only you can see this</strong></span>
			  		<% end %>
			 </div>
			 <hr>
    	</div>
     <div class="col-md-4">
    	<%= image_tag("underline.jpg") %>
    	<p style="font-size: 19px;color: brown;padding: 0px 0px 0px 0px;"><span style="color: white"> Photo Date:</span> <%= @upload.created_at.strftime("%B %d, 20%y") %></p>
    	<%= image_tag("underline.jpg") %>
    	<div class="likeornotholder">
    	
			<hr>
			
            <%= image_tag("underline.jpg") %>
            <div class="next-pev">
                <ul>
                    <% if @upload.next_upload %>
                    <li class="next-box"><%= link_to("Previous Photo", @upload.next_upload, :class => "")  %></li>
                    <% end %>
                    <% if @upload.previous_upload %>
                    <li class="next-box"><%= link_to("Next Photo", @upload.previous_upload,:class => "")  %></li>
                    <% end %>

                </ul>

            </div>
    </div>
